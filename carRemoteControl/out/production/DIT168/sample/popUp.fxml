<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="172.0" prefWidth="303.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.V2VConnect">
   <children>

       <Label layoutX="81.0" layoutY="40.0" text="Please enter the car ID:" />

       <TextField fx:id="carIdLabel"
                  layoutX="68.0"
                  layoutY="74.0" />

       <Button fx:id="connect"
              layoutX="117.0"
              layoutY="114.0"
              mnemonicParsing="false"
              text="Connect"
              onMouseClicked="#windowAction"/>

       <Label fx:id="errorMessage"
             layoutX="70.0"
             layoutY="150.0"
             text="Connection failed! Try again."
             visible="false" />

   </children>
</AnchorPane>
